#!/usr/bin/gnuplot

plot_title=system("echo $plot_title")
data_file=system("echo $data_file")

set terminal pngcairo size 800,480

set noborder

set key left nobox
set style data lines

set grid ytics

set border 0

set format y '%.0s %c'

set xlabel "count"
set ylabel "time [ns]"

plot for [IDX=0:2] \
  data_file index IDX using 1:2 title columnheader(1) lw 3

